{
  "multipart": [
    {
      "when": { "up": "true" },
      "apply": { "model": "etfuturum:polished_deepslate_wall_post" }
    },
    {
      "when": { "north": "true" },
      "apply": { "model": "etfuturum:polished_deepslate_wall_side", "uvlock": true }
    },
    {
      "when": { "east": "true" },
      "apply": { "model": "etfuturum:polished_deepslate_wall_side", "y": 90, "uvlock": true }
    },
    {
      "when": { "south": "true" },
      "apply": { "model": "etfuturum:polished_deepslate_wall_side", "y": 180, "uvlock": true }
    },
    {
      "when": { "west": "true" },
      "apply": { "model": "etfuturum:polished_deepslate_wall_side", "y": 270, "uvlock": true }
    }
  ]
}
